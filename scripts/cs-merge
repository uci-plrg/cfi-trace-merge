#!/bin/bash

. $csc/scripts/cs-common

function print_usage() {
  echo "Usage: cs-merge <left-run-dir> <right-run-dir> <log-file-basename>"
  exit
}

check_help $1 print_usage

append_classpath "CROWD_SAFE_MERGE_DIR"
append_classpath "CROWD_SAFE_COMMON_DIR"

# echo "classpath: $cp"

main_class="edu.uci.eecs.crowdsafe.merge.graph.main.MergeTwoExecutionGraphs"

java -cp $cp $main_class $@
  
